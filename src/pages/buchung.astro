---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import CatchHeader from '../components/bootcamp/CatchHeader.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';
---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
	</head>
	<body>
		<Header />
		<main>
            <CatchHeader topic="Eversports" headline="Buchung" />
            <div class="container section">
            <iframe src="https://www.eversports.de/widget/w/M7xnfH" width="100%" height="1450px" frameborder="0">
            </iframe>
        </div>
		</main>
		<Footer />
	</body>
</html>